{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "cdconstruct.com.au"
        }
      ],
      "destination": "https://www.cdconstruct.com.au/:path*",
      "permanent": true
    }
  ]
}
